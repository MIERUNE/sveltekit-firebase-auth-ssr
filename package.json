{
	"name": "@mierune/sveltekit-firebase-auth-ssr",
	"version": "0.0.3",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "npm run build && wrangler pages dev",
		"package": "svelte-kit sync && svelte-package -i src/lib/firebase-auth && publint",
		"prepublishOnly": "npm run package",
		"deploy": "npm run build && wrangler pages deploy --branch=main",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.0.0 || ^5.0.0-next",
		"@sveltejs/kit": "^2.5.27",
		"firebase": "^10.13.1",
		"firebase-auth-cloudflare-workers-x509": "^2.0.8"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240903.0",
		"@playwright/test": "^1.28.1",
		"@stripe/stripe-js": "^4.4.0",
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/adapter-node": "^5.2.2",
		"@sveltejs/package": "^2.3.5",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@types/eslint": "^8.56.7",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"hono": "^4.5.11",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.2.10",
		"stripe": "^16.7.0",
		"svelte": "^5.0.0-next.244",
		"svelte-check": "^4.0.1",
		"svelte-stripe": "^1.2.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3",
		"vitest": "^2.0.5"
	},
	"overrides": {
		"svelte-stripe": {
			"svelte": "^5.0.0-next.1",
			"@stripe/stripe-js": "^4"
		}
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
